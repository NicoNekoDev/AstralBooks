    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_13_R2</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_12_R1</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_11_R1</artifactId>
