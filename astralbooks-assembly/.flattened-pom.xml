<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>ro.niconeko</groupId>
    <artifactId>astralbooks-parent</artifactId>
    <version>3.0.3-SNAPSHOT</version>
  </parent>
  <groupId>ro.niconeko</groupId>
  <artifactId>astralbooks-assembly</artifactId>
  <version>3.0.3-SNAPSHOT</version>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.deploy.skip>true</maven.deploy.skip>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
  </properties>
  <dependencies>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-core</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-api</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_20_R1</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_19_R3</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_19_R2</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_19_R1</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_18_R2</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_18_R1</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_17_R1</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_16_R1</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_16_R2</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_16_R3</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
      <artifactId>astralbooks-v1_15_R1</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>ro.niconeko</groupId>
